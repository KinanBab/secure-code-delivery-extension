{
  "manifest_version": 2,

  "name": "Federated Code Delivery",
  "version": "1.0",
  
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  
  "web_accessible_resources": [
    "consensus.html",
    "consensus-script.js",
    "mpc.html",
    "mpc-script.js"
  ],

  
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "<all_urls>"
  ]
}